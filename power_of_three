#include<bits/stdc++.h>
#define ll long long
#define ull unsigned long long
using namespace std;
int main(){
    ll n,i,j;
    while(true){
    cin>>n;
    if(n<=0)
        cout<<"fasle"<<endl;
    else{
        while(n%3==0){
            n/=3;
        }
        if(n==1)
            cout<<"true"<<endl;
        else
            cout<<"false"<<endl;
        }
    }
    return 0;
}


//problem LeetCode power of three

class Solution {
public:
    bool isPowerOfThree(int n) {
        if(n<=0)
            return false;
        while(n%3==0){
            n/=3;
        }
        if(n==1)
            return true;
        else
            return false;
    }
};
