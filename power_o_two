#include<bits/stdc++.h>
using namespace std;
int main(){
    int i,j,n,x;
    while(true){
        cin>>n;
        int cnt=0;
        while(n!=0){
            if(n&1)
                cnt++;
            n = n>>1;
        }
        if(cnt==1)
            cout<<"yes"<<endl;
        else
            cout<<"NO"<<endl;
    }
return 0;
}
